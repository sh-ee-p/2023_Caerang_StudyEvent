    "while True:\n",
    "    nL = list(map(int, input()))\n",
    "    if nL==[0]:\n",
    "        break\n",
    "    cnt = 0\n",
    "    for i in nL:\n",
    "        if 1==i:\n",
    "            cnt += 2\n",
    "        elif 0==i:\n",
    "            cnt += 4\n",
    "        else:\n",
    "            cnt += 3\n",
    "    print(cnt+len(nL)-1+2)"
   ]
